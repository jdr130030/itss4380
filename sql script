create table PROJECT (
    PROJECT_ID number not null constraint project_pk primary key,
    CUS_ID number not null,
    EMP_ID number not null,
    PROJ_DATE date,
    DESCRIPTION varchar2(200),
    ES_START date,
    ES_END date,
    ES_BUDGET number,
    AC_START date,
    AC_END date,
    AC_COST number
);


