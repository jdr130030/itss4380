create table EMPLOYEE (
	EMP_ID number not null,
	REGION_ID varchar2(2) not null, 
	EMP_LNAME varchar2(20) not null,
	EMP_FNAME varchar2(20) not null,
	EMP_MI varchar2(5),
	EMP_HIREDATE date,
	PRIMARY KEY(EMP_ID)
	);
	



